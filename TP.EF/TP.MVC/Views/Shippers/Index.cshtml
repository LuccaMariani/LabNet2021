@model List<TP.MVC.Models.ShippersView>
@{
	ViewBag.Title = "Shippers Page";
}

<h1>Index de Shippers</h1>

<table class="table table-hover">
	<thead>
		<tr>
			<th scope="col">ID</th>
			<th scope="col">Name</th>

		</tr>
	</thead>
	<tbody>
		@foreach (var shipper in Model)
		{
			<tr>
				<th scope="row">@shipper.Id</th>
				<td>@shipper.Description</td>
			</tr>
		}
	</tbody>

</table>

<div>
	@Html.ActionLink("Insertar una nueva Region", "Insert", "Shippers")
</div>
